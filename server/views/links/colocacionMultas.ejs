    <div class="card">
      <div class="card-header">
        <img src="imagenes/digesettLogo.png"></img>
        <h3>Acta de Infracción</h3>

        <div class="custom-control custom-label custom-control-inline">
          <label class="" for="fecha">   Fecha:</label>
        <script>

        var mydate=new Date();
        var year=mydate.getYear();
        if (year < 1000)
        year+=1900;
        var day=mydate.getDay();
        var month=mydate.getMonth()+1;
        if (month<10)
        month="0"+month;
        var daym=mydate.getDate();
        if (daym<10)
        daym="0"+daym;
        document.write("<small><font color='white' face='Times New Roman' size='4'>"+daym+"/"+month+"/"+year+"</font></small>")
        </script>
      </div>
      <div class="custom-control custom-label custom-control-inline">
        <label class="" for="hora">Hora: </label>
        <script>
        var hora = new Date()
        var hrs = hora.getHours();
        var min = hora.getMinutes();
        document.write("<small><font color='white' face='Times New Roman' size='4' >"+hrs+":"+min+ "</font></small>")
        </script>
    </div>

      </div>

      <div class="card-body">
        <form>

          <div class="form-row">
              <div class="col-md-3">
                <label for="primernombre">Primer nombre:</label>
                <div class="input-group form-group">

                  <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fas fa-clock"></i></span>
                  </div>
                  <input type="text" class="form-control" id="primernombre" placeholder="">
                </div>

              </div>

              <div class="form-group col-md-3">
                <label for="segundonombre">Segundo nombre:</label>
                <input type="text" class="form-control" id="segundonombre" placeholder="">
              </div>
              <div class="form-group col-md-3">
                  <label for="primerapellido">Primer apellido:</label>
                  <input type="text" class="form-control" id="primerapellido" placeholder="">
              </div>
              <div class="form-group col-md-3">
                <label for="segundoapellido">Segundo apellido:</label>
                <input type="text" class="form-control" id="segundoapellido" placeholder="">
              </div>

              </div>

              <div class="form-row">
                <div class="form-group col-md-3">
                  <label for="cedula">Cédula:</label>



                  <input type="text" class="form-control" id="cedula">
                </div>


















              <div class="form-group col-md-3">
                <label for="telefonoconductor">Teléfono:</label>
                <input type="text" class="form-control" id="telefonoconductor">
              </div>
              <div class="form-group col-md-3">
                <label for="nacionalidad">Nacionalidad:</label>
                <select id="nacionalidad" class="form-control">
                  <option selected>Elegir...</option>
                  <option>Dominicano</option>
                  <option>Extranjero</option>
                </select>
              </div>
            <div class="form-group col-md-3">
              <label for="pais">País:</label>
              <input type="text" class="form-control" id="pais" placeholder="">
            </div>
          </div>

    <div class="form-row">
    <div class="form-group col-md-3">
      <label for="marca">Marca:</label>
